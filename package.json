{
  "name": "instant-v0",
  "displayName": "instant-v0",
  "description": "quickly use v0 on your project from vscode",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.94.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "instant-v0.v0",
        "title": "Open New v0 chat"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "pnpm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "lint": "eslint src"
  },
  "packageManager": "pnpm@9.2.0",
  "dependencies": {
    "playwright": "^1.48.1"
  },
  "devDependencies": {
    "@types/node": "20.x",
    "@types/vscode": "^1.94.0",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.7.0",
    "eslint": "^9.13.0",
    "typescript": "^5.6.3"
  }
}
